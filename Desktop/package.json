{
  "name": "ApolReady",
  "version": "1.0.0",
  "description": "An Apologetics Card Formater for Speech and Debate Students",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . ApolReady --overwrite --asar=true --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ApolReady\"",
    "package-linux": "electron-packager . ApolReady --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "repository": "https://github.com/Aaron-Mann/ApolReady",
  "keywords": [
    "ApolReady",
    "Apologetics"
  ],
  "author": "Aaron Mann",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.4"
  },
  "dependencies": {
    "electron-packager": "^13.1.1",
    "pdfkit": "^0.9.0"
  }
}
